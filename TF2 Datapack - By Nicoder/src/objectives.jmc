class Objectives
{
    class control_point
    {
        class find_index {
            function main() {
                // Looks up (using macros) the storage address of the current control point and the game it belongs to.

                // Storage format of `control_points::info[$local...][tf2.index:@s]`:
                //      owner: RawTextComponent (font = tf2:square)
                //          Team-colored square to show who controls the point. Flickers when opponents are attacking the point.
                //      progress: RawTextComponent (font = tf2:square)
                //          Team-color triangle that shows how close is point is to being taken. Grows left-to-right for BLU progress, and right-to-left for RED progress.
                //      symbol: RawTextComponent (font = tf2:square)
                //          Most commonly a locked indictator, but also includes the star for cp_steel's final point, and any other special symbols for specific maps.
                //      extra: RawTextComponent (font = tf2:square)
                //          Blank by default. If tf2.letter tag is present, displays the letter of the control point in sequence.
                //
                //      wheel.owner: RawTextComponent (font = tf2:wheel)
                //          Team-color circle in title to show who controls the point. Only shown to players on the point.
                //      wheel.progress: RawTextComponent (font = tf2:wheel)
                //          Team-color wedge sector in title to show how close is point is to being taken. Only shown to players on the point.
                //      wheel.spacing: RawTextComponent (font = tf2:wheel)
                //          Space characters used to make sure the wheel indicator points to the correct control point.

                // @output `lookup::points[0]`
                //  Dynamically-editable copy of all data in the `control_points::info[$local...][tf2.index:@s]` storage path.
                //  Can be copied back to the original path using `Objectives.control_point.find_index.reinsert()`

                index::i = tf2.batch_number:$local;
                index::j = 0.1 * (int) tf2.index:@s;
                $_access_mode_ = 1;
                this.private() with index::;
            } function reinsert() {
                // Copies the edited data at `tf2:lookup points[0]` back to the matching address in `control_points::info[$local...][@s tf2.index]`

                index::i = tf2.batch_number:$local;
                index::j = 0.1 * (int) tf2.index:@s;
                $_access_mode_ = 2;
                this.private() with index::;
            }
            function private() {
                $execute if score $_access_mode_ tf2.var matches 1 run lookup::points[0] = control_points::info[$(i)][$(j)];
                $execute if score $_access_mode_ tf2.var matches 2 run control_points::info[$(i)][$(j)] = lookup::points[0];
            }
        }

        function check_for_players() {
            // Counts the number of RED/BLU players on the control point.

            tag @s remove tf2.contested;

            execute positioned ~-3 ~-1 ~-3 as @e[type=#tf2:player_like,dx=5,dy=1,dz=5] run Player.determine_cap_rules();
            tag @a[gamemode=spectator] remove tf2.on_point;

            $red_on_point = execute if entity @e[tag=tf2.on_point, tag=tf2.can_cap, scores={tf2.team=1}];
            $red_scouts = execute if entity @e[tag=tf2.on_point, tag=tf2.can_cap, scores={tf2.team=1,tf2.class=Class.SCOUT}];
            $red_on_point += $red_scouts;

            $blu_on_point = execute if entity @e[tag=tf2.on_point, tag=tf2.can_cap, scores={tf2.team=2}];
            $blu_scouts = execute if entity @e[tag=tf2.on_point, tag=tf2.can_cap, scores={tf2.team=2,tf2.class=Class.SCOUT}];
            $blu_on_point += $blu_scouts;

            $red_defend = execute if entity @e[tag=tf2.on_point, tag=tf2.can_defend, scores={tf2.team=1}];
            $blu_defend = execute if entity @e[tag=tf2.on_point, tag=tf2.can_defend, scores={tf2.team=2}];

            if ($red_on_point && $blu_defend) {
                tag @s add tf2.contested;
            }
            if ($blu_on_point && $red_defend) {
                tag @s add tf2.contested;
            }

            if (!entity @s[tag=tf2.contested]) {
                this.main();
            }   
            tag @e[type=#tf2:player_like] remove tf2.on_point;
        }
        function main() {
            // Handles capture progress & associated visuals for the current control point.

            // Amount of capture progress = Nth partial sum of harmonic series, where N is how many players are standing on the point.
            $bound = $red_on_point;
            $bound > $blu_on_point;
            $bound < 4;
            Math.harmonic_series();
            if (tf2.team:@s != 0) {$sum *= 2;}

            // apply capture progress to team that has players on point, but you can't capture a point you already own
            if ($red_on_point && tf2.team:@s != 1) this.capture_progress.red();
            if ($blu_on_point && tf2.team:@s != 2) this.capture_progress.blu();

            if (tf2.team:@s == 2 && tf2.blu_progress:@s < tf2.capture_threshold:@s) this.visuals.capture_progress.red.main();
            if (tf2.team:@s == 0 && tf2.blu_progress:@s == 0) this.visuals.capture_progress.red.main();

            if (tf2.team:@s == 1 && tf2.red_progress:@s < tf2.capture_threshold:@s) this.visuals.capture_progress.blu.main();
            if (tf2.team:@s == 0 && tf2.red_progress:@s == 0) this.visuals.capture_progress.blu.main();

            this.decay.init();
        }
        function gamestate() {
            // Handles game progress & checks for win conditions. Executed as the gamestate marker, not a control point.
            // @within __tick__()

            tag @e remove tf2.current;
            tf2.batch_number:$local = tf2.batch_number:@s;
            execute as @e[type=item_display,tag=tf2.control_point]
					if score @s tf2.batch_number = $local tf2.batch_number
					run  tag @s add tf2.current;

            $red_cp_count = execute if entity @e[type=item_display,tag=tf2.current,scores={tf2.team=1}];
            $blu_cp_count = execute if entity @e[type=item_display,tag=tf2.current,scores={tf2.team=2}];
            // printf("Red = &<$red_cp_count>, Blu = &<$blu_cp_count>");
            // printf("index = &<tf2.index:@s>");
            // no need to count total number of control points, as that is already given by `tf2.index` [see `Objectives.control_point.visuals.spacing()` for more info]

            scoreboard players reset $winner tf2.var;
            if ($red_cp_count == tf2.index:@s[tag=!tf2.multistage,scores={tf2.gamemode=Gamemode.CONTROL_PTS}]) {
                $winner = 1;
            }
            if ($blu_cp_count == tf2.index:@s[tag=!tf2.multistage,scores={tf2.gamemode=Gamemode.ATK_DEF .. Gamemode.CONTROL_PTS}]) {
                $winner = 2;
            }
            // execute if score $red_cp_count tf2.var = @s tf2.index if score @s tf2.gamemode matches 2 if entity @s[tag=tf2.multistage] run function tf2:start_next_stage");
            // execute if score $blu_cp_count tf2.var = @s tf2.index if score @s tf2.gamemode matches 1..2 if entity @s[tag=tf2.multistage] run function tf2:start_next_stage");

            // Negative time means the game is already over but players haven't been relogged yet. `$winner == $winner` checks if score is set.
            if ($winner == $winner && tf2.timer:@s > -1) reset.announce_winner();

            tag @e[type=item_display,tag=tf2.control_point,tag=tf2.current] remove tf2.current;
        }
        function cancel_overtime() {
            tag @s remove tf2.overtime;
            index::i = tf2.batch_number:@s;
            tf2.batch_number:$local = tf2.batch_number:@s;
            execute as @e[type=marker,tag=tf2.in_use] at @s
	    	        if score @s tf2.batch_number = $local tf2.batch_number
					run Timer.cancel_overtime() with index::;
        }
        function lock() {
            tag @s add tf2.locked;
            tf2.red_progress:@s[scores={tf2.team=1}] = tf2.capture_threshold:@s;
            tf2.blu_progress:@s[scores={tf2.team=1}] = 0;
            tf2.red_progress:@s[scores={tf2.team=2}] = 0;
            tf2.blu_progress:@s[scores={tf2.team=2}] = tf2.capture_threshold:@s;
            this.find_index.main();
            if (tf2.team:@s == 1) lookup::points[0].owner = [{"text":"[","color":"#F5EACB"},{"text":"3210=","color":"#D95A58"}];
            if (tf2.team:@s == 2) lookup::points[0].owner = [{"text":"[","color":"#F5EACB"},{"text":"3210=","color":"#517AB8"}];
            lookup::points[0].progress = [{"text":"321))))))))))))))","color":"gray"}];
            lookup::points[0].symbol = [{"text":"320L@","font":"tf2:square"}];
            this.find_index.reinsert();
        }
        function unlock() {
            tag @s remove tf2.locked;
            this.find_index.main();
            lookup::points[0].symbol = [{"text":"320#)@","font":"tf2:square"}];
            this.find_index.reinsert();
        }

        class capture_progress {
            function red() {
                // @within:
                //      Objectives.control_point.main()
                //      Objectives.control_point.decay.main()

                // BLU progress must be reverted before adding RED progress
                tf2.blu_progress:@s[scores={tf2.blu_progress=1..}] -= $sum;
                tf2.blu_progress:@s > 0;
                tf2.red_progress:@s[scores={tf2.blu_progress=0}] += $sum;
                if (tf2.red_progress:@s >= tf2.capture_threshold:@s) Objectives.control_point.set_owner.red();

                // 50:50 chance to pick one of two dialogue lines
                $temp = execute if predicate tf2:coin_flip;
                if (entity @s[tag=!tf2.said_dialogue,tag=!tf2.last,scores={tf2.team=2}] && $temp = 0) Text.tellraw(@a[scores={tf2.team=2}], "&<bold>Alert! Our control point is being captured.");
                if (entity @s[tag=!tf2.said_dialogue,tag=!tf2.last,scores={tf2.team=2}] && $temp = 1) Text.tellraw(@a[scores={tf2.team=2}], "&<bold>Defend our control point!");
                if (entity @s[tag=!tf2.said_dialogue,tag=tf2.last,scores={tf2.team=2}] && $temp = 0) Text.tellraw(@a[scores={tf2.team=2}], "&<bold>Alert! Our &<bold,italic>last &<bold,!italic>control point is being captured!");
                if (entity @s[tag=!tf2.said_dialogue,tag=tf2.last,scores={tf2.team=2}] && $temp = 1) Text.tellraw(@a[scores={tf2.team=2}], "&<bold>Alert! The &<bold,italic>final &<bold,!italic>control point is under attack.");
                tag @s[scores={tf2.team=2}] add tf2.said_dialogue;
            } function blu() {
                // @within:
                //      Objectives.control_point.main()
                //      Objectives.control_point.decay.main()

                // RED progress must be reverted before adding BLU progress
                tf2.red_progress:@s[scores={tf2.red_progress=1..}] -= $sum;
                tf2.red_progress:@s > 0;
                tf2.blu_progress:@s[scores={tf2.red_progress=0}] += $sum;
                if (tf2.blu_progress:@s >= tf2.capture_threshold:@s) Objectives.control_point.set_owner.blu();

                // 50:50 chance to pick one of two dialogue lines
                $temp = execute if predicate tf2:coin_flip;
                if (entity @s[tag=!tf2.said_dialogue,tag=!tf2.last,scores={tf2.team=1}] && $temp = 0) Text.tellraw(@a[scores={tf2.team=1}], "&<bold>Alert! Our control point is being captured.");
                if (entity @s[tag=!tf2.said_dialogue,tag=!tf2.last,scores={tf2.team=1}] && $temp = 1) Text.tellraw(@a[scores={tf2.team=1}], "&<bold>Defend our control point!");
                if (entity @s[tag=!tf2.said_dialogue,tag=tf2.last,scores={tf2.team=1}] && $temp = 0) Text.tellraw(@a[scores={tf2.team=1}], "&<bold>Alert! Our &<bold,italic>last &<bold,!italic>control point is being captured!");
                if (entity @s[tag=!tf2.said_dialogue,tag=tf2.last,scores={tf2.team=1}] && $temp = 1) Text.tellraw(@a[scores={tf2.team=1}], "&<bold>Alert! The &<bold,italic>final &<bold,!italic>control point is under attack.");
                tag @s[scores={tf2.team=1}] add tf2.said_dialogue;
            }
        }
        class decay {
            function init() {
                // @within Objectives.control_point.check_for_players()

                // if non-owner team still has players on point, don't decay
                if ($red_on_point && tf2.team:@s != 1) return 0;
                if ($blu_on_point && tf2.team:@s != 2) return 0;
                this.main();
            }
            function main() {
                // @within this.init()

                $sum = tf2.capture_threshold:@s;
                $sum /= 1800; // Takes 90 seconds to fully revert progress.
                if (tf2.team:@s != 0) $sum *= 2;
                if (entity @s[tag=tf2.overtime]) $sum *= 6;
                tf2.batch_number:$local = tf2.batch_number:@s;
                execute as @e[type=marker,tag=tf2.in_use] if score $local tf2.batch_number = @s tf2.batch_number run tag @s add tf2.current;

                // revert in direction of owner team
                if (tf2.team:@s == 1) Objectives.control_point.capture_progress.red();
                if (tf2.team:@s == 2) Objectives.control_point.capture_progress.blu();

                if (score @s[tag=tf2.overtime,scores={tf2.team=1}] tf2.red_progress = @s tf2.capture_threshold) execute as @e[tag=tf2.current,limit=1] run Timer.end_game();
                if (score @s[tag=tf2.overtime,scores={tf2.team=2}] tf2.blu_progress = @s tf2.capture_threshold) execute as @e[tag=tf2.current,limit=1] run Timer.end_game();

                // if unowned, revert towards zero
                tf2.red_progress:@s[scores={tf2.team=0,tf2.red_progress=1..}] -= $sum;
                tf2.blu_progress:@s[scores={tf2.team=0,tf2.blu_progress=1..}] -= $sum;
                tf2.red_progress:@s > 0;
                tf2.blu_progress:@s > 0;

                tag @s[scores={tf2.team=0,tf2.red_progress=0,tf2.blu_progress=0}] remove tf2.said_dialogue;
                tag @e remove tf2.current;
            }
        }
        class on_capture {
            class red {
                function symmetric() {
                    tf2.index:$temp = tf2.index:@s;
                    tf2.index:$temp -= 10;

                    execute as @e[type=item_display,tag=tf2.control_point,tag=tf2.current,scores={tf2.team=2}]
							if score @s tf2.index = $temp tf2.index
							at @s run Objectives.control_point.unlock();

                    if (tf2.team:$last = 0) return 0;
                    tf2.index:$temp += 20;
                    execute as @e[type=item_display,tag=tf2.control_point,tag=tf2.current,scores={tf2.team=1}]
							if score @s tf2.index = $temp tf2.index
							at @s run Objectives.control_point.lock();

                    // TODO: add support for map events (like doors opening)
                }
                function koth() {
                    tf2.team:@e[type=marker,tag=tf2.current,tag=tf2.batch] = 1;
                    execute as @a if score @s tf2.batch_number = $local tf2.batch_number run tag @s add tf2.current;
                    $temp = execute if predicate tf2:coin_flip;
                    if ($temp) Text.tellraw(@a[tag=tf2.current,scores={tf2.team=1}], "&<bold,red>We have captured the control point!");
                    if (!$temp) Text.tellraw(@a[tag=tf2.current,scores={tf2.team=1}], "&<bold,red>We have secured the control point!");
                    Text.tellraw(@a[tag=tf2.current,scores={tf2.team=2}], "&<bold,blue>We have lost the control point!");
                    tag @a remove tf2.current;
                }
            } class blu {
                function atk_def() {
                    tf2.index:$temp = tf2.index:@s;
                    tf2.index:$temp += 10;

                    execute at @s run Objectives.control_point.lock();
                    execute as @e[type=item_display,tag=tf2.control_point,tag=tf2.current]
							if score @s tf2.index = $temp tf2.index
							at @s run Objectives.control_point.unlock();

                    // TODO: add support for map events (like doors opening)
                }
				function symmetric() {
                    tf2.index:$temp = tf2.index:@s;
                    tf2.index:$temp += 10;

                    execute as @e[type=item_display,tag=tf2.control_point,tag=tf2.current,scores={tf2.team=1}]
							if score @s tf2.index = $temp tf2.index
					 		at @s run Objectives.control_point.unlock();

                    if (tf2.team:$last = 0) return 0;
                    tf2.index:$temp -= 20;
                    execute as @e[type=item_display,tag=tf2.control_point,tag=tf2.current,scores={tf2.team=2}]
							if score @s tf2.index = $temp tf2.index
							at @s run Objectives.control_point.lock();

                    // TODO: add support for map events (like doors opening)
                }
                function koth() {
                    tf2.team:@e[type=marker,tag=tf2.current,tag=tf2.batch] = 2;
                    execute as @a if score @s tf2.batch_number = $local tf2.batch_number run tag @s add tf2.current;
                    $temp = execute if predicate tf2:coin_flip;
                    if ($temp) Text.tellraw(@a[tag=tf2.current,scores={tf2.team=2}], "&<bold,blue>We have captured the control point!");
                    if (!$temp) Text.tellraw(@a[tag=tf2.current,scores={tf2.team=2}], "&<bold,blue>We have secured the control point!");
                    Text.tellraw(@a[tag=tf2.current,scores={tf2.team=1}], "&<bold,red>We have lost the control point!");
                    tag @a remove tf2.current;
                }
            }
            function add_time() {
                tf2.timer:@e[type=marker,tag=tf2.batch,tag=tf2.current,limit=1] += tf2.increment:@s;
                tf2.timer:@e[type=marker,tag=tf2.batch,tag=tf2.current,limit=1,scores={tf2.gamemode=Gamemode.CONTROL_PTS}] = 12000;

                execute as @a if score @s tf2.batch_number = $local tf2.batch_number run tag @s add tf2.current;
                if (tf2.red_progress:@s >= tf2.capture_threshold:@s) expand {
                    Text.tellraw(@a[tag=tf2.current,scores={tf2.team=1}], "&<bold,red>Success. Additional time has been awarded.");
                    Text.tellraw(@a[tag=tf2.current,scores={tf2.team=2}], "&<bold,blue>The &<bold,italic,blue>enemy &<bold,blue>has received additional time.");
                }
                if (tf2.blu_progress:@s >= tf2.capture_threshold:@s) expand {
                    Text.tellraw(@a[tag=tf2.current,scores={tf2.team=1}], "&<bold,red>The &<bold,italic,red>enemy &<bold,red>has received additional time.");
                    Text.tellraw(@a[tag=tf2.current,scores={tf2.team=2}], "&<bold,blue>Success. Additional time has been awarded.");
                }
            }
        }
        class map_events {
            function handle() {
                index::i = tf2.batch_number:$local;
                index::j = 0.1 * (int) tf2.index:@s;
                execute run {
                    $ lookup::events = running_games::$(i).objectives[$(j)].data.events;
                } with index::;

                Hardcode.repeatList("`prop`", () => {
                    function spawn_`prop`() {
                        // default value of 0 means it will do nothing if unspecified
                        index::red = 0;
                        index::red = lookup::events.spawn_`prop`.red;
                        index::blu = 0;
                        index::blu = lookup::events.spawn_`prop`.blu;
                        execute run {
                            $ running_games::$(i).spawn_`prop`.red[0] = running_games::$(i).spawn_`prop`.red[$(red)];
                            $ running_games::$(i).spawn_`prop`.blu[0] = running_games::$(i).spawn_`prop`.blu[$(blu)];
                        } with index::;
                    }
                    if (data storage tf2:lookup {events:{spawn_`prop`:{}}}) {this.spawn_`prop`();}
                }, strings=["point", "time"]);

                while (data storage tf2:lookup events.doors[]) {
                    tf2.door.id:$i = lookup::events.doors[-1].id;
                    // printf("&<yellow>Checking for door #&<tf2.door.id:$i>...");
                    execute as @e[type=item_display, tag=tf2.door]
                            if score @s tf2.batch_number = $local tf2.batch_number
                            if score @s tf2.door.id = $i tf2.door.id
                            run {
                                // printf("&<green> Hi, this is door #&<tf2.door.id:@s>");
                                // printf("Prev door state: &<tf2.door.state:@s>");
                                tf2.door.state:@s = lookup::events.doors[-1].state;
                                // printf("New door state: &<tf2.door.state:@s>");
                            }
                    lookup::events.doors[-1].del();
                }
            }
        }
        class set_owner {
            function red() {
                // @within Objectives.control_point.capture_progress.red()

                // Keep track of the team that *previously* controlled this point.
                tf2.team:$last = tf2.team:@s;
                tf2.team:@s = 1;
                tf2.red_progress:@s = tf2.capture_threshold:@s;

                Objectives.control_point.find_index.main();
                lookup::points[0].owner = [{"text":"[","color":"#F5EACB"},{"text":"3210=","color":"#D95A58"}];
                lookup::points[0].progress = [{"text":"321))))))))))))))","color":"gray"}];
                Objectives.control_point.find_index.reinsert();
                tag @s remove tf2.said_dialogue;

                // Update model to be RED
                item modify entity @s contents {
                    "function": "minecraft:set_custom_model_data",
                    "floats": { "values": [1], "mode": "replace_all" }
                };

                
                tf2.batch_number:$local = tf2.batch_number:@s;
                Hardcode.repeatList("`type`", () => {
                    tag @e[type=`type`,tag=tf2.current] remove tf2.current;
                    execute as @e[type=`type`] 
                            if score @s tf2.batch_number = $local tf2.batch_number 
                            run tag @s add tf2.current;
                }, strings=["marker", "item_display"]);

                if (tf2.team:$last == tf2.team:@s) return 0;

                Objectives.control_point.map_events.handle() with index::;

                if (entity @s[tag=tf2.overtime]) {
                    Objectives.control_point.cancel_overtime();
                }
                // No ATF/DEF branch because that is impossible
                if (tf2.gamemode:@s[tag=!tf2.multistage] == Gamemode.CONTROL_PTS) {
                    // lock/unlock adjacent points in line
                    Objectives.control_point.on_capture.red.symmetric();
                }
                if (tf2.gamemode:@s == Gamemode.KOTH) {
                    // start counting down RED's timer & pause BLU's
                    Objectives.control_point.on_capture.red.koth();
                }
                if (tf2.increment:@s) {
                    Objectives.control_point.on_capture.add_time();
                }
            }
            function blu() {
                // @within Objectives.control_point.capture_progress.blu()

                // Keep track of the team that *previously* controlled this point.
                tf2.team:$last = tf2.team:@s;
                tf2.team:@s = 2;
                tf2.blu_progress:@s = tf2.capture_threshold:@s;

                Objectives.control_point.find_index.main();
                lookup::points[0].owner = [{"text":"[","color":"#F5EACB"},{"text":"3210=","color":"#517AB8"}];
                lookup::points[0].progress = [{"text":"321))))))))))))))","color":"gray"}];
                Objectives.control_point.find_index.reinsert();
                tag @s remove tf2.said_dialogue;

                // Update model to be BLU
                item modify entity @s contents {
                    "function": "minecraft:set_custom_model_data",
                    "floats": { "values": [2], "mode": "replace_all" }
                };


                tf2.batch_number:$local = tf2.batch_number:@s;
                Hardcode.repeatList("`type`", () => {
                    tag @e[type=`type`,tag=tf2.current] remove tf2.current;
                    execute as @e[type=`type`] 
                            if score @s tf2.batch_number = $local tf2.batch_number 
                            run tag @s add tf2.current;
                }, strings=["marker", "item_display"]);
                
                if (tf2.team:$last == tf2.team:@s) return 0;
                
                Objectives.control_point.map_events.handle() with index::;

                if (entity @s[tag=tf2.overtime]) {
                    Objectives.control_point.cancel_overtime();
                }
                if (tf2.gamemode:@s == Gamemode.ATK_DEF) {
                    // lock this point to solidify progress, & unlock next point 
                    Objectives.control_point.on_capture.blu.atk_def();
                }
                if (tf2.gamemode:@s[tag=!tf2.multistage] == Gamemode.CONTROL_PTS) {
                    // lock/unlock adjacent points in line
                    Objectives.control_point.on_capture.blu.symmetric();
                }
                if (tf2.gamemode:@s == Gamemode.KOTH) {
                    // start counting down BLU's timer & pause RED's
                    Objectives.control_point.on_capture.blu.koth();
                }
                if (tf2.increment:@s) {
                    Objectives.control_point.on_capture.add_time();
                }
            }

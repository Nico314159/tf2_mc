        class visuals {
            function init() {
                // Sets all visual data relating to this point's state, based on the start condition of the map.
                // @within __tick__()

                tf2.batch_number:$local = tf2.batch_number:@s;
                Objectives.control_point.find_index.main();

                // Set background color in HUD.
                Hardcode.repeatLists(
                    ["_num_", "_color_", "_hex_"],
                    () => {
                        if (tf2.team:@s == _num_) {
                            lookup::points[0].owner = [{"text":"[","color":"#F5EACB"},{"text":"3210=","color":"#_hex_"}];
                        }
                    },
                    [["0", "1", "2"], ["white", "red", "blue"], ["A1A1A1", "D95A58", "517AB8"]]
                );
                lookup::points[0].progress = [{"text":"321))))))))))))))","color":"gray"}];

                // Determine the custom model to use.
                item modify entity @s contents {
                    "function": "minecraft:set_custom_model_data",
                    "floats": {
                        "values": [
                            {
                                "type": "minecraft:score",
                                "target": "this",
                                "score": "tf2.team"
                            }
                        ],
                        "mode": "replace_all"
                    }
                };

                this.add_symbol();
                if (entity @s[tag=!tf2.letter]) {
					lookup::points[0].extra = [{"text":"$#)","font":"tf2:square"}];
				}

                // Puts a letter in the corner of the HUD icon, if the `tf2.letter` tag is present.
                Hardcode.repeatLists(
                    ["_num_", "_letter_"],
                    () => {
                        if (tf2.index:@s[tag=tf2.letter] == _num_) lookup::points[0].extra = [{"text":"3210|]","color":"gray"},{"text":"|21_letter_$#","color":"black"}];
                    },
                    [["0", "10", "20", "30", "40"], ["A", "B", "C", "D", "E"]]
                );
                Objectives.control_point.find_index.reinsert();
                tag @s add tf2.init;
            }
            function add_symbol() {
				// @within this.init()
                if (entity @s[tag=tf2.locked]) {
	                return run lookup::points[0].symbol = [{"text":"320L@","font":"tf2:square"}];
    		    }
                if (entity @s[tag=tf2.star]) {
    	        	return run lookup::points[0].symbol = [{"text":"321S!)","font":"tf2:square"}];
                }
                lookup::points[0].symbol = [""];
            }
